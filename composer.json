{
  "name": "vragovr/safecrow-bundle",
  "description": "SafeCrow API Bundle",
  "version": "1.0.0",
  "type": "library",
  "keywords": [
    "symfony",
    "bundle",
    "api",
    "SafeCrow"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Vragov",
      "email": "vragovR@yandex.ru"
    }
  ],
  "require": {
